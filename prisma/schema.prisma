generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ratings {
  id        Int     @id @default(autoincrement())
  user_id   Int?
  movie_id  Int?
  recommend String?
  seen      String?
}

model rating {
  id        Int     @id @default(autoincrement())
  user_id   String?
  movie_id  Int?
  recommend String?
  seen      String?
}
